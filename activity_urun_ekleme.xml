<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UrunEklemeActivity">

    <Button
        android:id="@+id/urunListeButon"
        android:layout_width="60dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#FF9800"
        android:background="#FF9800"
        android:drawableBottom="@mipmap/liste"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="165dp" />

    <Button
        android:id="@+id/aramaButon"
        android:layout_width="60dp"
        android:layout_height="55dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="113dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#FF9800"
        android:background="#FF9800"
        android:drawableBottom="@mipmap/ara"/>

    <Button
        android:id="@+id/bildirimButon"
        android:layout_width="60dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#FF9800"
        android:background="#FF9800"
        android:drawableBottom="@mipmap/bildirim" />

    <Button
        android:id="@+id/urunEkleButon"
        android:layout_width="60dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="50dp"
        android:backgroundTint="#FF9800"
        android:background="#FF9800"
        android:drawableBottom="@mipmap/ekle" />

    <ListView
        android:id="@+id/listele"
        android:layout_width="match_parent"
        android:layout_height="225dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp" />


</RelativeLayout>